{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "env": {
    "node": true,
    "es6": true
  },
  "ignorePatterns": ["node_modules/", "script/", "doc/", "coverage/", "uploads-textcontent.js"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 11
  },
  "rules": {
    "no-alert": 2,
    "no-array-constructor": 1,
    "no-bitwise": 0,
    "no-caller": 1,
    "no-case-declarations": 2,
    "no-catch-shadow": 2,
    "no-class-assign": 1,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-continue": 1,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 0,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-else-return": 1,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 0,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 0,
    "no-implied-eval": 1,
    "no-inline-comments": 0,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-invalid-this": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 0,
    "no-label-var": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 1,
    "no-loop-func": 0,
    "no-mixed-requires": [0, { "grouping": true }],
    "no-mixed-spaces-and-tabs": [0, false],
    "linebreak-style": [0, "unix"],
    "no-multi-spaces": 0,
    "no-multi-str": 0,
    "no-multiple-empty-lines": [0, { "max": 2 }],
    "no-native-reassign": 2,
    "no-negated-condition": 0,
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 2,
    "no-new": 0,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-require": 1,
    "no-new-wrappers": 1,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 0,
    "no-param-reassign": 0,
    "no-path-concat": 0,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-proto": 0,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-imports": 0,
    "no-restricted-modules": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 1,
    "no-script-url": 0,
    "no-self-compare": 2,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 0,
    "no-sparse-arrays": 2,
    "no-sync": 0,
    "no-ternary": 0,
    "no-this-before-super": 0,
    "no-throw-literal": 0,
    "no-undef": 2,
    "no-undef-init": 1,
    "no-undefined": 1,
    "no-unexpected-multiline": 0,
    "no-unmodified-loop-condition": 0,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 1,
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "no-use-before-define": 0,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 0,
    "no-var": 1,
    "no-warning-comments": [0, { "terms": ["todo", "fixme", "xxx"], "location": "start" }],
    "no-with": 2,
    "no-magic-numbers": 0,
    "array-bracket-spacing": [0, "never"],
    "array-callback-return": 0,
    "arrow-body-style": [0, "as-needed"],
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "accessor-pairs": 0,
    "block-scoped-var": 0,
    "block-spacing": 0,
    "brace-style": [0, "1tbs"],
    "camelcase": 1,
    "comma-spacing": 0,
    "comma-style": 0,
    "complexity": [2, 20],
    "computed-property-spacing": [0, "never"],
    "consistent-return": 0,
    "consistent-this": [0, "that"],
    "constructor-super": 0,
    "curly": [0, "all"],
    "default-case": 1,
    "dot-location": 0,
    "dot-notation": [1, { "allowKeywords": true }],
    "eqeqeq": 2,
    "func-names": 0,
    "func-style": [0, "declaration"],
    "generator-star-spacing": 0,
    "global-require": 0,
    "guard-for-in": 2,
    "handle-callback-err": 0,
    "id-length": [2, { "min": 1, "max": 40 }],
    "indent": 0,
    "init-declarations": 0,
    "jsx-quotes": [0, "prefer-double"],
    "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
    "lines-around-comment": 0,
    "max-depth": [2, 5],
    "max-nested-callbacks": [2, 10],
    "max-params": [2, 10],
    "max-statements": [2, 50],
    "new-cap": 0,
    "new-parens": 0,
    "newline-after-var": 0,
    "object-curly-spacing": [0, "never"],
    "object-shorthand": 0,
    "one-var": [2, "never"],
    "operator-assignment": [0, "always"],
    "operator-linebreak": 0,
    "padded-blocks": [0, "never"],
    "prefer-const": 2,
    "prefer-reflect": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "quote-props": 0,
    "quotes": [0, "single"],
    "radix": 0,
    "id-match": 0,
    "require-jsdoc": 0,
    "require-yield": 0,
    "sort-vars": 0,
    "space-after-keywords": [0, "always"],
    "space-before-keywords": [0, "always"],
    "space-before-blocks": [0, "always"],
    "space-before-function-paren": [0, "always"],
    "space-in-parens": [0, "never"],
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": [0, { "words": true, "nonwords": false }],
    "spaced-comment": 0,
    "strict": [2, "global"],
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "wrap-regex": 0,
    "yield-star-spacing": 0,
    "yoda": [0, "never"],
    "jasmine/no-focused-tests": 0,
    // airbnb rules that we will need to respect in a near futur
    "no-underscore-dangle": ["warn", { "allow": ["_id", "__v"], "allowAfterThis": true }],
    "no-await-in-loop": 0,
    "no-useless-catch": "warn",
    "class-methods-use-this": 0,
    "prefer-promise-reject-errors": "warn",
    "import/order": "warn",
    "no-cond-assign": "warn",
    "no-restricted-globals": "warn",
    "no-prototype-builtins": "warn",
    "no-useless-escape": "warn",
    "prefer-destructuring": "warn",
    "callback-return": ["warn", ["cb", "callback", "next"]],
    "no-useless-constructor": "warn",
    "max-classes-per-file": "warn",
    "no-empty-function": "warn",
    "no-return-await": "warn",
    "valid-typeof": "warn",
    "no-async-promise-executor": "warn"
  },
  "globals": {
    "$": false,
    "global": false,
    "jQuery": false,
    "headers": false,
    "require": false,
    "jasmine": false,
    "describe": false,
    "xdescribe": false,
    "it": false,
    "xit": false,
    "expect": false,
    "beforeEach": false,
    "afterEach": false,
    "waitsFor": false,
    "runs": false,
    "spyOn": false,
    "fail": false,
    "controllers": true,
    "utils": true,
    "arrayUtil": true,
    "brandController": true,
    "configController": true,
    "portalController": true,
    "functionsUtils": true,
    "apiUrl": true,
    "request": true
  }
}
